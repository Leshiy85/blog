<!DOCTYPE html>
<html>
<head>
  <title>КліНіка</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header class="navbar navbar-fixed-top navbar-default">
    <div class="container">
      <%= link_to "КліНіка", "", class:"btn btn-lg btn-success", id: "logo" %>
      <div class="nav_item">
        <%= link_to "HOME", "", class: "btn btn-default btn-warning" %>
      </div>
       <div class="nav_item">
        <%= link_to "BLOG", "", class: "btn btn-default btn-warning" %>
      </div>
       <div class="nav_item">
        <%= link_to "GALLERY", "", class: "btn btn-default btn-warning" %>
      </div>
       <div class="nav_item">
        <%= link_to "CONTACT", "", class: "btn btn-default btn-warning" %>
      </div>
    </div>
  </header>

<%= yield %>

</body>
</html>
